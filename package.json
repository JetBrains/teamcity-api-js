{
  "name": "@jetbrains/teamcity-ui",
  "version": "1.0.0-alpha.1",
  "description": "",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*/index.d.ts",
      "import": "./dist/*/index.mjs",
      "require": "./dist/*/index.js"
    },
    "./*/*": {
      "types": "./dist/*/*.d.ts",
      "import": "./dist/*/*.mjs",
      "require": "./dist/*/*.js"
    },
    "./index.css": "./dist/index.css",
    "./babel.config": "./babel.config.js",
    "./getWebpackConfig": "./getWebpackConfig.js",
    "./plugin.tsconfig.json": "./plugin.tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JetBrains/teamcity-ui.git"
  },
  "homepage": "https://github.com/JetBrains/teamcity-ui#readme",
  "keywords": [
    "teamcity",
    "jetbrains"
  ],
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "license": "MIT",
  "workspaces": [
    "browserslist"
  ],
  "dependencies": {
    "@jetbrains/icons": "^5.8.0",
    "@jetbrains/ring-ui-built": "^7.0.36",
    "change-case": "^5.4.4",
    "classnames": "^2.5.1",
    "react-is": "^19.1.0",
    "react-markdown": "^10.1.0",
    "util-deprecate": "^1.0.2",
    "whatwg-fetch": "^3.6.20"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-export-default-from": "^7.25.9",
    "@babel/plugin-transform-runtime": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.27.0",
    "@chromatic-com/storybook": "^3.2.6",
    "@eslint/compat": "^1.2.8",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.24.0",
    "@jetbrains/eslint-config": "^6.0.4",
    "@jetbrains/ring-ui": "^7.0.38",
    "@storybook/addon-essentials": "^8.6.11",
    "@storybook/addon-interactions": "^8.6.11",
    "@storybook/addon-onboarding": "^8.6.11",
    "@storybook/blocks": "^8.6.11",
    "@storybook/react": "^8.6.11",
    "@storybook/react-vite": "^8.6.11",
    "@storybook/test": "^8.6.11",
    "@vitejs/plugin-react": "^4.3.4",
    "browserslist": "^4.24.4",
    "browserslist-to-esbuild": "^2.1.1",
    "commitizen": "4.3.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-import-resolver-typescript": "^4.3.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "from": "^0.1.7",
    "import": "^0.0.6",
    "js": "^0.1.0",
    "prettier": "^3.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "standard-version": "9.5.0",
    "storybook": "^8.6.11",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.29.1",
    "vite": "^6.2.4",
    "vite-css-modules": "^1.8.4",
    "vite-plugin-commonjs": "^0.10.4",
    "vite-plugin-dts": "^4.5.3",
    "vite-plugin-dynamic-import": "^1.6.0"
  },
  "peerDependencies": {
    "@jetbrains/ring-ui": ">=4.2.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "webpack": ">=5.0.0",
    "webpack-bundle-analyzer": ">=4.0.0"
  },
  "peerDependenciesMeta": {
    "@jetbrains/ring-ui": {
      "optional": true
    },
    "webpack": {
      "optional": true
    },
    "webpack-bundle-analyzer": {
      "optional": true
    }
  },
  "scripts": {
    "lint": "eslint .",
    "lint-fix": "npm run lint -- --fix",
    "typecheck": "tsc --noEmit",
    "build": "vite build",
    "postbuild": "tsc",
    "commit": "git-cz",
    "version:ci": "standard-version",
    "prerelease:ci": "npm run lint && npm run build",
    "release:ci": "git push --follow-tags origin master && npm publish",
    "storybook": "storybook dev -p 6007",
    "build-storybook": "storybook build"
  },
  "browserslist": [
    "extends @jetbrains/browserslist-config-teamcity"
  ]
}
